#include <bits/stdc++.h>
using namespace std;


int main() {
	// code goes here
	int T;
	scanf("%d",&T);
	while(T--){
	    int n=0,t;
	    for(int i=0;i<3;i++){
	        cin>>t;
	        if(t==7){
	            n=1;
	           
	        }
	        
	    }
	    if(n==1){
	        cout<<"YES"<<endl;
	    }
	    else{
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
