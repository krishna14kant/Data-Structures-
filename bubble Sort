void sortColors(int* nums, int numsSize) {
    for(int i=1;i<=numsSize-1;i++)
    {
       for(int j=0;j<=numsSize-2;j++)
       {
           if(nums[j]>nums[j+1])
           {
              int temp=nums[j+1];
               nums[j+1]=nums[j];
               nums[j]=temp;
               
           }
           
       }
        
    }
}
